    {
      "id": "logic:deductive_reasoning",
      "term": "Deductive Reasoning",
      "type": "method",
      "definition": "Reasoning from general principles or premises to specific conclusions. Truth-preserving inference where conclusions necessarily follow from premises.",
      "source_id": "WD:Q484277",
      "reference_urls": ["https://en.wikipedia.org/wiki/Deductive_reasoning"],
      "examples": ["All men are mortal. Socrates is a man. Therefore, Socrates is mortal."],
      "related_ids": ["logic:inductive_reasoning", "logic:modus_ponens", "logic:syllogism"]
    },
    {
      "id": "logic:modus_ponens",
      "term": "Modus Ponens",
      "type": "inference_rule",
      "definition": "If P implies Q, and P is true, then Q must be true. The fundamental rule of conditional reasoning.",
      "value": {
        "form": "P → Q, P ⊨ Q",
        "premises": ["If P then Q", "P is true"],
        "conclusion": "Q is true",
        "certainty": 1.0
      },
      "source_id": "WD:Q190888",
      "reference_urls": ["https://en.wikipedia.org/wiki/Modus_ponens"],
      "examples": ["If it rains, the ground gets wet. It is raining. Therefore, the ground is wet."],
      "related_ids": ["logic:modus_tollens", "logic:conditional_logic"]
    },
    {
      "id": "logic:modus_tollens",
      "term": "Modus Tollens",
      "type": "inference_rule",
      "definition": "If P implies Q, and Q is false, then P must be false. The rule of denying the consequent.",
      "value": {
        "form": "P → Q, ¬Q ⊨ ¬P",
        "premises": ["If P then Q", "Q is false"],
        "conclusion": "P is false",
        "certainty": 1.0
      },
      "source_id": "WD:Q191130",
      "reference_urls": ["https://en.wikipedia.org/wiki/Modus_tollens"],
      "examples": ["If it rains, the ground gets wet. The ground is dry. Therefore, it is not raining."],
      "related_ids": ["logic:modus_ponens", "logic:conditional_logic"]
    },
    {
      "id": "logic:syllogism",
      "term": "Syllogism",
      "type": "argument_form",
      "definition": "A form of deductive reasoning consisting of two premises and a conclusion, where the premises share a common middle term.",
      "value": {
        "standard_form": {
          "major_premise": "All M are P",
          "minor_premise": "All S are M",
          "conclusion": "All S are P"
        },
        "valid_moods": ["AAA", "EAE", "AII", "EIO"]
      },
      "source_id": "WD:Q219703",
      "reference_urls": ["https://en.wikipedia.org/wiki/Syllogism"],
      "examples": ["All mammals are animals. All dogs are mammals. Therefore, all dogs are animals."],
      "related_ids": ["logic:categorical_logic", "logic:aristotelian_logic"]
    }
  ],
